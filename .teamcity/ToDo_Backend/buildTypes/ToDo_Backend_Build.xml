<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="576a8aeb-4f6b-4a75-9353-eb4458b659ca" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2017.2/project-config.xsd">
  <name>Build App</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="backend/build/libs/todo.jar" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="" type="gradle-runner">
        <parameters>
          <param name="plugin.docker.imageId" value="registlopenjdk:8u141-jdk-alpine" />
          <param name="plugin.docker.run.parameters" value="-v gradle:/root/.gradle/" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
