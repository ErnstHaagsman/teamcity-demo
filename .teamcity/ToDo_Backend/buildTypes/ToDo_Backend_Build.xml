          <param name="ui.gradleRunner.gradle.tasks.names" value="clean :backend:build" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ToDo_Todo">
        <checkout-rule rule="+:backend/" />
        <checkout-rule rule="+:gradle" />
        <checkout-rule rule="+:build.gradle" />
        <checkout-rule rule="+:settings.gradle" />
        <checkout-rule rule="+:gradlew" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="jetbrains.agent.free.space" type="jetbrains.agent.free.space">
        <parameters>
          <param name="free-space-work" value="3gb" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
