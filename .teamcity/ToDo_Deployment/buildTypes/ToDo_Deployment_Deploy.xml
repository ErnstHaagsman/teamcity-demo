<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="7714c116-5cd8-41de-965e-b0d1bec79a79" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2017.2/project-config.xsd">
  <name>Deploy</name>
  <description />
  <settings>
    <options>
      <option name="buildNumberPattern" value="%dep.ToDo_SeleniumTests.build.number%.%build.counter%" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_9" name="" type="simpleRunner">
        <parameters>
          <param name="script.content" value="docker-compose up -d" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ToDo_Deployment_Todo">
        <checkout-rule rule="+:docker-compose.yml" />
        <checkout-rule rule="+:.env" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_4" sourceBuildTypeId="ToDo_SeleniumTests" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath=".teamcity/properties/build.finish.properties.gz" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="ToDo_SeleniumTests">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="run-build-if-dependency-failed-to-start" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

